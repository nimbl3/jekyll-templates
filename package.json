    "stimulus": "2.0.0",
    "style-loader": "2.0.0",
    "svg-spritemap-webpack-plugin": "4.0.3",
    "tailwindcss": "2.1.4",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0",
    "webpack-merge": "5.7.3",
    "webpack-shell-plugin-next": "2.2.2"
